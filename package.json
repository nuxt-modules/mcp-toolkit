{
  "name": "nuxt-mcp-toolkit-monorepo",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/nuxt-mcp-toolkit.git"
  },
  "author": "Hugo Richard <hugo.richard@vercel.com>",
  "license": "MIT",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "turbo dev --filter=nuxt-mcp-toolkit-playground",
    "dev:prepare": "turbo dev:prepare",
    "dev:docs": "turbo dev --filter=nuxt-mcp-toolkit-docs",
    "build": "turbo build",
    "build:docs": "turbo build --filter=nuxt-mcp-toolkit-docs",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "lint:docs": "turbo lint --filter=nuxt-mcp-toolkit-docs",
    "test": "turbo test",
    "typecheck": "turbo typecheck",
    "typecheck:docs": "turbo typecheck --filter=nuxt-mcp-toolkit-docs",
    "clean": "turbo clean"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.10.0",
    "eslint": "^9.39.1",
    "turbo": "^2.6.1",
    "typescript": "~5.9.3"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
