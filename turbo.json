{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".output/**",
        ".nuxt/**"
      ],
      "env": [
        "STUDIO_GITHUB_CLIENT_ID",
        "STUDIO_GITHUB_CLIENT_SECRET"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "STUDIO_GITHUB_CLIENT_ID",
        "STUDIO_GITHUB_CLIENT_SECRET"
      ]
    },
    "dev:prepare": {
      "cache": false,
      "outputs": [
        "dist/**",
        ".output/**",
        ".nuxt/**"
      ]
    },
    "lint": {
      "cache": true
    },
    "lint:fix": {
      "cache": false
    },
    "test": {
      "cache": true
    },
    "typecheck": {
      "cache": true
    },
    "eval": {
      "cache": false
    },
    "eval:ui": {
      "cache": false,
      "persistent": true
    }
  }
}
